<h1>{{title}}</h1>
<ul>
  {{#blogs}}
    <li>
    <h2><a href="/api/blog/{{id}}">{{title}}</a></h2> 
    	<p>{{description}}</p>
    	<p>Written by: {{createdBy}} | On: {{createdAt}}
    	<div>Likes:{{likes}}</div>
    	<a href="/api/deleteBlog/{{id}}/{{createdBy}}">Delete this Blog</a>
    </li>
  {{/blogs}}
</ul>

<div><a href="/api/addBlog">ADD BLOG</a></div>